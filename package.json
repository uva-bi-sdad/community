{
  "name": "community",
  "version": "0.0.1",
  "description": "Base assets for community data sites.",
  "main": "dev/community.js",
  "directories": {
    "doc": "docs",
    "man": "man",
    "test": "tests/js"
  },
  "scripts": {
    "start": "rollup --config --watch --bundleConfigAsCjs",
    "build": "rollup --config rollup.prod.js --bundleConfigAsCjs",
    "build-stable": "rollup --config rollup.prod.js --environment community_dist_version:v2 --bundleConfigAsCjs",
    "test": "npx jest --config jest.config.js",
    "find-deadcode": "ts-prune"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uva-bi-sdad/community.git"
  },
  "author": "Biocomplexity Institute",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uva-bi-sdad/community/issues"
  },
  "homepage": "https://uva-bi-sdad.github.io/community",
  "devDependencies": {
    "@jest/globals": "latest",
    "@rollup/plugin-terser": "latest",
    "@rollup/plugin-typescript": "latest",
    "@types/datatables.net": "latest",
    "@types/jest": "latest",
    "@types/leaflet": "latest",
    "csso": "latest",
    "jest": "latest",
    "jest-environment-jsdom": "latest",
    "rollup": "latest",
    "rollup-plugin-serve": "latest",
    "ts-jest": "latest",
    "ts-prune": "latest",
    "tslib": "latest",
    "typescript": "latest"
  }
}
